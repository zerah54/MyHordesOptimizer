{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MyHordesOptimizerWebsite": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "mho",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "preserveSymlinks": true,
            "outputPath": "dist/MyHordesOptimizerWebsite/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/img",
              "src/robots.txt"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/scss/material-theme/theming.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss",
                "src/assets/scss/material-theme",
                "src/assets/scss/noel"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "100mb",
                  "maximumError": "500mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "100mb",
                  "maximumError": "500mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "MyHordesOptimizerWebsite:build:production"
            },
            "development": {
              "browserTarget": "MyHordesOptimizerWebsite:build:development"
            },
            "staging": {
              "browserTarget": "MyHordesOptimizerWebsite:build:staging"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MyHordesOptimizerWebsite:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js.testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/img"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss",
                "src/assets/scss/material-theme",
                "src/assets/scss/noel"
              ]
            },
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/MyHordesOptimizerWebsite/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss",
                "src/assets/scss/**/"
              ]
            },
            "preserveSymlinks": true,
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            },
            "staging": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "production": {
              "browserTarget": "MyHordesOptimizerWebsite:build:production",
              "serverTarget": "MyHordesOptimizerWebsite:server:production"
            },
            "development": {
              "browserTarget": "MyHordesOptimizerWebsite:build:development",
              "serverTarget": "MyHordesOptimizerWebsite:server:development"
            },
            "staging": {
              "browserTarget": "MyHordesOptimizerWebsite:build:staging",
              "serverTarget": "MyHordesOptimizerWebsite:server:staging"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "MyHordesOptimizerWebsite:build:production",
              "serverTarget": "MyHordesOptimizerWebsite:server:production"
            },
            "development": {
              "browserTarget": "MyHordesOptimizerWebsite:build:development",
              "serverTarget": "MyHordesOptimizerWebsite:server:development"
            },
            "staging": {
              "browserTarget": "MyHordesOptimizerWebsite:build:staging",
              "serverTarget": "MyHordesOptimizerWebsite:server:staging"
            }
          },
          "defaultConfiguration": "production"
        }
      },
      "i18n": {
        "sourceLocale": "fr",
        "locales": {
          "de": "src/assets/i18n/xlf-files/messages.de.xlf",
          "en": "src/assets/i18n/xlf-files/messages.en.xlf",
          "es": "src/assets/i18n/xlf-files/messages.es.xlf"
        }
      }
    }
  }
}
