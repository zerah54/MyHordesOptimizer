{
    "name": "my-hordes-optimizer-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build && npm run sentry:sourcemaps",
        "build:staging": "ng build --configuration staging",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "extract-i18n": "ng extract-i18n MyHordesOptimizerWebsite --format xlf --output-path src/i18n && xliffmerge --profile xliffmerge-options.json",
        "build-deploy": "ng build && firebase use prod && firebase deploy --only hosting:prod",
        "build-deploy:dev": "ng build --configuration staging && firebase use dev && firebase deploy --only hosting:dev",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org myhordes-optimizer --project frontend ./dist && sentry-cli sourcemaps upload --org myhordes-optimizer --project frontend ./dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "19.1.7",
        "@angular/cdk": "19.1.5",
        "@angular/common": "19.1.7",
        "@angular/compiler": "19.1.7",
        "@angular/core": "19.1.7",
        "@angular/fire": "19.0.0",
        "@angular/forms": "19.1.7",
        "@angular/localize": "19.1.7",
        "@angular/material": "19.1.5",
        "@angular/platform-browser": "19.1.7",
        "@angular/platform-browser-dynamic": "19.1.7",
        "@angular/platform-server": "19.1.7",
        "@angular/router": "19.1.7",
        "@angular/ssr": "19.1.8",
        "@fortawesome/fontawesome-free": "6.7.2",
        "@kolkov/angular-editor": "3.0.0-beta.2",
        "@microsoft/signalr": "8.0.7",
        "@sentry/angular": "9.1.0",
        "@sentry/cli": "2.42.1",
        "chart.js": "4.4.8",
        "chartjs-plugin-datalabels": "2.2.0",
        "compass-mixins": "0.12.12",
        "express": "4.21.2",
        "localstorage-polyfill": "1.0.1",
        "material-icons": "1.13.14",
        "moment": "2.30.1",
        "moment-timezone": "0.5.47",
        "ng-table-virtual-scroll": "1.6.1",
        "rxjs": "7.8.2",
        "uuid": "11.1.0",
        "vis-data": "^7.1.9",
        "vis-network": "^9.1.9",
        "zone.js": "0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "19.1.8",
