{
    "name": "my-hordes-optimizer-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build && npm run sentry:sourcemaps",
        "build:staging": "ng build --configuration staging",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "extract-i18n": "ng extract-i18n MyHordesOptimizerWebsite --format xlf --output-path src/i18n && xliffmerge --profile xliffmerge-options.json",
        "build-deploy": "ng build && firebase use prod && firebase deploy --only hosting:prod",
        "build-deploy:dev": "ng build --configuration staging && firebase use dev && firebase deploy --only hosting:dev",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org myhordes-optimizer --project frontend ./dist && sentry-cli sourcemaps upload --org myhordes-optimizer --project frontend ./dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "18.2.3",
        "@angular/cdk": "18.2.3",
        "@angular/common": "18.2.3",
        "@angular/compiler": "18.2.3",
        "@angular/core": "18.2.3",
        "@angular/fire": "18.0.1",
        "@angular/forms": "18.2.3",
        "@angular/localize": "18.2.3",
        "@angular/material": "18.2.3",
        "@angular/platform-browser": "18.2.3",
        "@angular/platform-browser-dynamic": "18.2.3",
        "@angular/platform-server": "18.2.3",
        "@angular/router": "18.2.3",
        "@angular/ssr": "18.2.4",
        "@fortawesome/fontawesome-free": "6.6.0",
        "@kolkov/angular-editor": "3.0.0-beta.0",
        "@microsoft/signalr": "8.0.7",
        "@sentry/angular": "^9.0.1",
        "@sentry/cli": "^2.41.1",
        "chart.js": "4.4.4",
        "chartjs-plugin-datalabels": "2.2.0",
        "compass-mixins": "0.12.12",
        "express": "4.20.0",
        "localstorage-polyfill": "1.0.1",
        "material-icons": "1.13.12",
        "moment": "2.30.1",
        "moment-timezone": "0.5.45",
        "ng-table-virtual-scroll": "1.6.1",
        "rxjs": "7.8.1",
        "uuid": "10.0.0",
        "vis-data": "^7.1.9",
        "vis-network": "^9.1.9",
        "zone.js": "0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "18.2.4",
        "@angular-eslint/builder": "18.3.1",
        "@angular-eslint/eslint-plugin": "18.3.1",
        "@angular-eslint/eslint-plugin-template": "18.3.1",
        "@angular-eslint/template-parser": "18.3.1",
        "@angular/cli": "18.2.4",
        "@angular/compiler-cli": "18.2.3",
        "@types/chart.js": "2.9.41",
        "@types/express": "4.17.21",
        "@types/jasmine": "5.1.4",
        "@types/node": "22.5.4",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "8.5.0",
        "@typescript-eslint/parser": "8.5.0",
        "browser-sync": "3.0.2",
        "eslint": "9.10.0",
        "firebase": "10.13.1",
        "jasmine-core": "5.3.0",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
