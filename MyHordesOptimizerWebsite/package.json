{
    "name": "my-hordes-optimizer-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "xlftojson": "node src/i18n/scripts/xlf_to_json.js",
        "extract-i18n": "ng extract-i18n MyHordesOptimizerWebsite --format xlf --output-path src/i18n && xliffmerge --profile xliffmerge-options.json",
        "build-deploy": "node src/i18n/scripts/xlf_to_json.js && ng build && firebase use prod && firebase deploy --only hosting:prod",
        "build-deploy:dev": "node src/i18n/scripts/xlf_to_json.js && ng build --configuration test && firebase use dev && firebase deploy --only hosting:dev"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~16.1.7",
        "@angular/cdk": "~16.1.6",
        "@angular/common": "~16.1.7",
        "@angular/compiler": "~16.1.7",
        "@angular/core": "~16.1.7",
        "@angular/fire": "^7.6.1",
        "@angular/forms": "~16.1.7",
        "@angular/material": "~16.1.6",
        "@angular/platform-browser": "~16.1.7",
        "@angular/platform-browser-dynamic": "~16.1.7",
        "@angular/platform-server": "~16.1.7",
        "@angular/router": "~16.1.7",
        "@nguniversal/express-engine": "^16.1.1",
        "chart.js": "^4.3.2",
        "chartjs-plugin-datalabels": "^2.2.0",
        "express": "^4.18.2",
        "localstorage-polyfill": "^1.0.1",
        "moment": "^2.29.4",
        "ng-table-virtual-scroll": "^1.6.0",
        "rxjs": "~7.8.1",
        "zone.js": "~0.13.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~16.1.6",
        "@angular-eslint/builder": "^16.1.0",
        "@angular-eslint/eslint-plugin": "^16.1.0",
        "@angular-eslint/eslint-plugin-template": "^16.1.0",
        "@angular-eslint/template-parser": "^16.1.0",
        "@angular/cli": "~16.1.6",
        "@angular/compiler-cli": "~16.1.7",
        "@angular/localize": "~16.1.7",
        "@nguniversal/builders": "^16.1.0",
        "@types/chart.js": "~2.9.37",
        "@types/express": "~4.17.17",
        "@types/jasmine": "~4.3.5",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
        "firebase": "^10.1.0",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ngx-i18nsupport": "^0.17.1",
        "tslib": "^2.6.1",
        "typescript": "~5.1.6",
        "xliff": "^6.1.0",
        "xlsx": "^0.18.5"
    }
}
