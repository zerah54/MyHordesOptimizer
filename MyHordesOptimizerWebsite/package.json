        "@angular-eslint/eslint-plugin-template": "19.1.0",
        "@angular-eslint/template-parser": "19.1.0",
        "@angular/cli": "19.1.8",
        "@angular/compiler-cli": "19.1.7",
        "@types/chart.js": "2.9.41",
        "@types/express": "5.0.0",
        "@types/jasmine": "5.1.7",
        "@types/node": "22.13.5",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "8.24.1",
        "@typescript-eslint/parser": "8.24.1",
        "browser-sync": "3.0.3",
        "eslint": "9.21.0",
        "firebase": "11.3.1",
        "jasmine-core": "5.6.0",
        "karma": "6.4.4",
