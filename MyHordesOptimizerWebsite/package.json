{
  "name": "my-hordes-optimizer-website",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run sentry:sourcemaps",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extract-i18n": "ng extract-i18n MyHordesOptimizerWebsite --format xlf --output-path src/i18n && xliffmerge --profile xliffmerge-options.json",
    "build-deploy": "ng build && firebase use prod && firebase deploy --only hosting:prod",
    "build-deploy:dev": "ng build --configuration staging && firebase use dev && firebase deploy --only hosting:dev",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org myhordes-optimizer --project frontend ./dist && sentry-cli sourcemaps upload --org myhordes-optimizer --project frontend ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.4",
    "@angular/cdk": "20.0.3",
    "@angular/common": "20.0.4",
    "@angular/compiler": "20.0.4",
    "@angular/core": "20.0.4",
    "@angular/fire": "20.0.1",
    "@angular/forms": "20.0.4",
    "@angular/localize": "20.0.4",
    "@angular/material": "20.0.3",
    "@angular/platform-browser": "20.0.4",
    "@angular/platform-browser-dynamic": "20.0.4",
    "@angular/platform-server": "20.0.4",
    "@angular/router": "20.0.4",
    "@angular/ssr": "20.0.3",
    "@fortawesome/fontawesome-free": "6.7.2",
    "@kolkov/angular-editor": "3.0.0-beta.2",
    "@microsoft/signalr": "8.0.7",
    "@sentry/angular": "9.30.0",
    "@sentry/cli": "2.46.0",
    "chart.js": "4.5.0",
    "chartjs-plugin-datalabels": "2.2.0",
    "compass-mixins": "0.12.12",
    "express": "5.1.0",
    "localstorage-polyfill": "1.0.1",
    "material-icons": "1.13.14",
    "moment": "2.30.1",
    "moment-timezone": "0.6.0",
    "ng-table-virtual-scroll": "1.6.1",
    "rxjs": "7.8.2",
    "vis-data": "7.1.9",
    "vis-network": "9.1.12",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.3",
    "@angular-eslint/builder": "20.1.0",
    "@angular-eslint/eslint-plugin": "20.1.0",
    "@angular-eslint/eslint-plugin-template": "20.1.0",
    "@angular-eslint/template-parser": "20.1.0",
    "@angular/cli": "20.0.3",
    "@angular/compiler-cli": "20.0.4",
    "@types/chart.js": "2.9.41",
    "@types/express": "5.0.3",
    "@types/jasmine": "5.1.8",
    "@types/node": "24.0.3",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "browser-sync": "3.0.4",
    "buffer": "6.0.3",
    "eslint": "9.29.0",
    "firebase": "11.9.1",
    "jasmine-core": "5.8.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
