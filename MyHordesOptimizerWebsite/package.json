{
    "name": "my-hordes-optimizer-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:staging": "ng build --configuration staging",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "xlftojson": "node src/i18n/scripts/xlf_to_json.js",
        "extract-i18n": "ng extract-i18n MyHordesOptimizerWebsite --format xlf --output-path src/i18n && xliffmerge --profile xliffmerge-options.json",
        "build-deploy": "node src/i18n/scripts/xlf_to_json.js && ng build && firebase use prod && firebase deploy --only hosting:prod",
        "build-deploy:dev": "node src/i18n/scripts/xlf_to_json.js && ng build --configuration staging && firebase use dev && firebase deploy --only hosting:dev"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~17.1.0",
        "@angular/cdk": "~17.1.0",
        "@angular/common": "~17.1.0",
        "@angular/compiler": "~17.1.0",
        "@angular/core": "~17.1.0",
        "@angular/fire": "^17.0.1",
        "@angular/forms": "~17.1.0",
        "@angular/material": "~17.1.0",
        "@angular/platform-browser": "~17.1.0",
        "@angular/platform-browser-dynamic": "~17.1.0",
        "@angular/platform-server": "~17.1.0",
        "@angular/router": "~17.1.0",
        "@fortawesome/fontawesome-free": "^6.5.1",
        "@kolkov/angular-editor": "^3.0.0-beta.0",
        "chart.js": "^4.4.1",
        "chartjs-plugin-datalabels": "^2.2.0",
        "compass-mixins": "^0.12.12",
        "express": "^4.18.2",
        "localstorage-polyfill": "^1.0.1",
        "material-icons": "^1.13.12",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.44",
        "ng-table-virtual-scroll": "^1.6.0",
        "rxjs": "~7.8.1",
        "uuid": "^9.0.1",
        "zone.js": "^0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~17.1.0",
        "@angular-eslint/builder": "^17.2.1",
        "@angular-eslint/eslint-plugin": "^17.2.1",
        "@angular-eslint/eslint-plugin-template": "^17.2.1",
        "@angular-eslint/template-parser": "^17.2.1",
        "@angular/cli": "~17.1.0",
        "@angular/compiler-cli": "~17.1.0",
        "@angular/localize": "~17.1.0",
        "@types/chart.js": "~2.9.41",
        "@types/express": "~4.17.21",
        "@types/jasmine": "~5.1.4",
        "@types/node": "^20.11.5",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "eslint": "^8.56.0",
        "firebase": "^10.7.2",
        "jasmine-core": "~5.1.1",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ngx-i18nsupport": "^0.17.1",
        "tslib": "^2.6.2",
        "typescript": "~5.3.3",
        "xliff": "^6.2.0",
        "xlsx": "^0.18.5"
    }
}
